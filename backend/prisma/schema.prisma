datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Location {
  id        String  @id @default(uuid())
  name      String
  city      String
  address   String
  state     String
  country   String
  whatsapp  String?
  latitude  Float
  longitude Float
  createdAt DateTime @default(now())
}

